{
  "name": "temp-h1founders",
  "version": "0.5.0",
  "description": "A monorepo template built with Bun, Hono, Vite, and React",
  "author": "Steve Simkins",
  "license": "MIT",
  "homepage": "https://github.com/stevedylandev/bhvr",
  "packageManager": "bun@1.2.4",
  "workspaces": [
    "./server",
    "./client",
    "./shared"
  ],
  "scripts": {
    "dev": "turbo dev",
    "dev:client": "turbo dev --filter=client",
    "dev:server": "turbo dev --filter=server",
    "build": "turbo build",
    "build:client": "turbo build --filter=client",
    "build:server": "turbo build --filter=server",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "test:auth": "npx playwright test tests/phone-auth.spec.ts",
    "test:auth:debug": "npx playwright test tests/phone-auth.spec.ts --debug",
    "test:e2e": "npx playwright test",
    "postinstall": "turbo build --filter=shared --filter=server"
  },
  "keywords": [
    "bun",
    "hono",
    "react",
    "vite",
    "monorepo",
    "turbo"
  ],
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "bun-types": "latest",
    "csv-parse": "^6.1.0",
    "firebase-admin": "^13.5.0",
    "turbo": "^2.5.5"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  }
}
